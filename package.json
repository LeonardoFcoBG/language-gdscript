{
  "name": "language-gdscript",
  "version": "0.1.0",
  "description": "GDScript highlighting for Pulsar",
  "license": "MIT",
  "keywords": [
    "godot",
    "gdscript",
    "syntax",
    "godotengine"
  ],
  "repository": "https://github.com/LeonardoFcoBG/language-gdscript",
  "engines": {
		"atom": ">=1.0.0 <2.0.0"
  },
  "grammars":[
    {
      "fileTypes":["gd"],
      "name":"GDScript",
      "scopeName":"gdscript",
      "path":"./grammars/gdscript.cson"
    }
  ],
  "activationHooks":[
    "language-gdscript:grammar-used"
  ],
  "dependencies": {
  }
}
