{
  "name": "language-gdscript",
  "main": "./lib/language-gdscript",
  "version": "0.0.0",
  "description": "GDScript support for Pulsar",
  "keywords": [
    "godot",
    "gdscript",
    "syntax",
    "godotengine"
  ],
  "activationCommands": {
    "atom-workspace": "language-gdscript:toggle"
  },
  "repository": "https://github.com/LeonardoFcoBG/language-gdscript",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
    "pulsar":"*"
  },
  "grammars":[
    {
      "fileTypes":["gd"],
      "name":"GDScript",
      "scopeName":"source.gdscript",
      "path":"./grammars/gdscript.cson"
    }
  ],
  "dependencies": {
  }
}
